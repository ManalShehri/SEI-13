iltet the movie by the type \r\nconst onChangeHandler = (e) =>{\r\n    let value = e.target.value\r\n    if (value == \"All\") { // if the select all show all the movies \r\n        setSelectMovie(movies)\r\n    }else { // if not show only the movie type amd we use filter method ! \r\n        setSelectMovie(movies.filter(movie => movie.typee == value))\r\n    }\r\n\r\n}\r\n    return (\r\n        <div>\r\n            <Container >\r\n                <h1 className=\"mt-5\" style={{ textAlign: \"center\" }}>All movies</h1>\r\n\r\n                <Row className=\"justify-content-md-center\" >\r\n                    <Col md=\"4\" >\r\n                        <Form>\r\n                            <Form.Group controlId=\"exampleForm.SelectCustom\">\r\n                                <Form.Label>Type of the movie</Form.Label>\r\n                                <Form.Control as=\"select\" custom onChange ={onChangeHandler}>\r\n                                {allSelect}\r\n                                </Form.Control>\r\n                            </Form.Group>\r\n                        </Form>\r\n                    </Col>\r\n                </Row>\r\n                {/* movies cards */}\r\n                <Row className=\"justify-content-md-center\">\r\n                   \r\n                    {allmovies}\r\n                   \r\n                </Row>\r\n\r\n            </Container>\r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\yasse\\Desktop\\lastDayOfMErn\\react-movie-lab_router\\src\\components\\Signup.jsx",["70","71","72"],"C:\\Users\\yasse\\Desktop\\lastDayOfMErn\\react-movie-lab_router\\src\\components\\Login.jsx",[],"C:\\Users\\yasse\\Desktop\\lastDayOfMErn\\react-movie-lab_router\\src\\components\\UserProfile.jsx",["73","74","75"],"import React, { useEffect, useState } from \"react\";\r\nimport { Container, Row } from \"react-bootstrap\";\r\nimport { Route, Redirect } from \"react-router-dom\";\r\nimport OneCardMovie from \"./OneCardMovie\";\r\nimport axios from 'axios'\r\n\r\nexport default function UserProfile(props) {\r\n  const [alluserMovies, setAluserMovies] = useState([])\r\n  const [changeuseEffect, setChangeuseEffect] = useState(false)\r\n  const { name, email, _id } = props.auth.currentUser;\r\n\r\n  \r\n  useEffect(() => {\r\n    axios.get('https://sei12.herokuapp.com/movei/json')\r\n      .then(data => {\r\n        // \r\n        let favoriteMovies = localStorage[_id]\r\n        console.log(favoriteMovies)\r\n        let filtermovies = data.data.filter(movie => favoriteMovies.includes(movie._id))\r\n        setAluserMovies(filtermovies)\r\n      })\r\n\r\n  }, [changeuseEffect])\r\n  const deleteMovei = (movieId) => {\r\n    let userId = _id\r\n    axios.delete(`http://localhost:5000/api/movies/${movieId}/${userId}`)\r\n      .then(data => {\r\n        localStorage[_id] = JSON.stringify(data.data.favoriteMovies)\r\n        console.log(localStorage[_id])\r\n        setChangeuseEffect(!changeuseEffect)\r\n      })\r\n\r\n\r\n  }\r\n\r\n  const allfavoriteMovies = alluserMovies.map(movie => <OneCardMovie deleteMovei={deleteMovei} movie={movie} delete={true} />)\r\n  return <> <h1> name :{name} </h1>\r\n    <h1> email : {email} </h1>\r\n    <h1> favorite Movies : </h1>\r\n\r\n    <Container>\r\n      <Row >\r\n        {allfavoriteMovies}\r\n      </Row>\r\n\r\n    </Container>\r\n  </>\r\n\r\n}\r\n","C:\\Users\\yasse\\Desktop\\lastDayOfMErn\\react-movie-lab_router\\src\\components\\AuthRoute.jsx",[],"C:\\Users\\yasse\\Desktop\\lastDayOfMErn\\react-movie-lab_router\\src\\components\\About.jsx",[],"C:\\Users\\yasse\\Desktop\\lastDayOfMErn\\react-movie-lab_router\\src\\components\\ShowOneMovie.jsx",["76","77","78"],"import axios from 'axios'\r\nimport React, { useState,useEffect } from 'react'\r\nimport { Button, Col, Container, Row } from 'react-bootstrap'\r\nimport { propTypes } from 'react-bootstrap/esm/Image';\r\nimport {useParams} from 'react-router-dom';\r\n\r\n\r\nexport default function ShowOneMovie(pr